{
  "browserify": {
    "src": "src/js/app.js",
    "build": "dist/js/",
    "plugin": []
  },
  "html":{
    "src": "src/templates/**/*.html",
    "build": "dist/"
  },
  "media":{
    "src": "src/assets/**/*",
    "build": "dist/assets/"
  },
  "styles": {
    "build": "dist/assets/css",
    "sass": {
      "src": "src/styles/**/*.scss",
      "includePaths": [
      ]
    }
  },
  "vendor": {
    "require": [
      "angular-material",
      "angular-ui-router",
      "angular-material-data-table",
      "angular-aria",
      "angular-animate"
    ],
    "build": {
      "dirname": "dist/js/",
      "name": "vendor.js"
    }
  }
}
