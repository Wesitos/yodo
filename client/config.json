{
  "browserify": {
    "src": "src/js/",
    "extname": "js",
    "build": "dist/js/",
    "plugin": []
  },
  "html":{
    "src": "src/templates",
    "build": "dist/",
    "extname": "html"
  },
  "media":{
    "src": "src/media",
    "build": "dist/media/"
  },
  "styles": {
    "build": "dist/css",
    "sass": {
      "src": "src/styles",
      "extname": "scss",
      "includePaths": [
        "node_modules/materialize-css/sass"
      ]
    }
  },
  "vendor": {
    "require": [
      'angular-material',
      'angular-ui-router',
      'angular-material-data-table'
    ],
    "build": {
      "dirname": "dist/js/",
      "name": "vendor.js"
    }
  }
}
